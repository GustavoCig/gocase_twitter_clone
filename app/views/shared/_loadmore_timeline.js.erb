<% if !@tweets_reload.empty? %>
  $('#load-more-content')
    .replaceWith('<%= j render partial: "shared/tweets", collection: @tweets_reload, as: :tweet %> <div id="load-more-content"></div>');
<% else %>
  $('#load-more-content')
    .replaceWith('<div> No more new content to be loaded </div>')
<% end %>